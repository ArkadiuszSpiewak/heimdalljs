{
  "name": "heimdall",
  "version": "3.0.0",
  "description": "An application management portal",
  "productName": "Heimdall",
  "cordovaId": "site.heimdall.app",
  "capacitorId": "",
  "author": "KodeStar <kodestar@gmail.com>",
  "contributors": [
    "Alex Phillips <ahp118@gmail.com>"
  ],
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "server": "npm run db:migrate && node ./bin/www",
    "db:migrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "db:seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "pretest": "NODE_ENV=test npm run db:migrate",
    "test": "jest",
    "build": "node ./node_modules/@quasar/cli/bin/quasar build",
    "setup": "npm install && node ./scripts/install && npm run build && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@quasar/extras": "^1.7.0",
    "axios": "^0.18.1",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dockerode": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "file-type": "^14.2.0",
    "inquirer": "^7.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.27",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "qrcode": "^1.4.4",
    "quasar": "^1.9.16",
    "sequelize": "^5.21.7",
    "speakeasy": "^2.0.0",
    "sqlite3": "^4.2.0",
    "vue-async-computed": "^3.8.2",
    "vue-i18n": "^8.17.4",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@quasar/app": "^1.7.4",
    "@quasar/cli": "^1.0.7",
    "@quasar/quasar-app-extension-dotenv": "^1.0.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^25.5.2",
    "sequelize-cli": "^5.5.1"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
